                                                                    <li>
                                                                        <a href="/category/<%= category.slug %>">
                                                                            <%= category.name %>
                                                                        </a>
                                                                    </li>
                                                                <% }) %>
                                                            </ul>
                                                            <% } %>
                                                        </li>
                                                    <% }) %>
                                                </ul>
                                                <% } %>
                                            </li>
                                        <% }) %>
                                    </ul>
                                </div>
