{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "arrow-parens": "warn",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "func-names": "off"
    }
}
